<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
    <servlet-name>Servlet.StatsServlet</servlet-name>
    <servlet-class>servlet.StatsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet.StatsServlet</servlet-name>
    <url-pattern>/stats/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlet.MatchesServlet</servlet-name>
    <servlet-class>servlet.MatchesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet.MatchesServlet</servlet-name>
    <url-pattern>/matches/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlet.SwipeServlet</servlet-name>
    <servlet-class>servlet.SwipeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet.SwipeServlet</servlet-name>
    <url-pattern>/swipe/*</url-pattern>
  </servlet-mapping>
</web-app>